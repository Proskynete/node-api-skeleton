{
  "name": "node-api-skeleton",
  "version": "0.0.1",
  "description": "skeleton for your node api projects",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev ./src/server.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\" \"test/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\" \"test/**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "cross-env PORT=5054 jest --forceExit --detectOpenHandles --runInBand --verbose",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "typescript-eslint": "^8.19.1"
  },
  "keywords": [
    "Nodejs",
    "Express",
    "TypeScript",
    "ESlint",
    "Prettier",
    "Husky",
    "Jest",
    "Swagger"
  ]
}
